--
-- Table structure for table `yiiapp_doc_events`
--

DROP TABLE IF EXISTS `yiiapp_doc_events`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `yiiapp_doc_events` (
  `Id` int(11) NOT NULL AUTO_INCREMENT,
  `Subject` varchar(200) NOT NULL COMMENT 'Тема',
  `eventNumber` varchar(50) CHARACTER SET utf8 NOT NULL COMMENT 'вутренний номер события',
  `author` int(11) DEFAULT NULL COMMENT 'id автора события',
  `responsible` int(11) DEFAULT NULL,
  `coexecutor` int(11) DEFAULT NULL,
  `Notes` text NOT NULL COMMENT 'Заметки',
  `Place` varchar(200) NOT NULL COMMENT 'Место',
  `EventTypeId` varchar(200) NOT NULL COMMENT 'Тип События',
  `organizationId` int(11) NOT NULL COMMENT 'ID организации',
  `Begin` datetime NOT NULL COMMENT 'Начало',
  `End` datetime NOT NULL COMMENT 'Окончание',
  `contractorId` int(11) DEFAULT NULL COMMENT 'идентификатор контрагента',
  `contractId` int(11) NOT NULL COMMENT 'идентификатор договора',
  `Percentage` float NOT NULL COMMENT 'Процент выполнения',
  `totalSum` int(11) NOT NULL COMMENT 'сумма события/заказа',
  `ReflectInCalendar` int(11) NOT NULL COMMENT 'Отражать в календаре',
  `ParentId` int(11) NOT NULL COMMENT 'Родитель',
  `Priority` int(11) NOT NULL COMMENT 'Приоритет',
  `StatusId` int(11) NOT NULL COMMENT 'Статус',
  `Comment` varchar(200) NOT NULL COMMENT 'Комментарий',
  `PlanHours` int(11) NOT NULL COMMENT 'Планируется часов',
  `FactHours` float NOT NULL COMMENT 'фактически часов',
  `Tags` varchar(200) NOT NULL COMMENT 'Тэги',
  `manualTransactionEditing` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'ручное редактирование проводок',
  `monitoringstatus` varchar(250) NOT NULL COMMENT 'контроль состояния',
  `Slugba` int(250) NOT NULL,
  `SrokPlan` datetime NOT NULL,
  `SrokFact` datetime NOT NULL,
  `Razdel` varchar(255) NOT NULL,
  `Podrazdel` varchar(255) NOT NULL,
  `Target` text NOT NULL,
  PRIMARY KEY (`Id`)
) ENGINE=MyISAM AUTO_INCREMENT=217 DEFAULT CHARSET=cp1251 COMMENT='События';
/*!40101 SET character_set_client = @saved_cs_client */;

