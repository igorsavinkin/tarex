д = \"000023564\";\r\n\r\nГруппаТелевизоры = Справочники.Номенклатура.НайтиПоКоду(\"000000085\");\r\n\r\n//если нужно записать элемент в корень справочника, то тогда Родитель не заполняется\r\n\r\nНовыйЭлемент.Родитель = ГруппаТелевизоры; \r\nНовыйЭлемент.ЕдиницаИзмерения = Справочники.ЕдиницыИзмерения.НайтиПоНаименованию(\"шт\");\r\n\r\n// далее заполняются все необходимые реквизиты\r\n\r\nНовыйЭлемент.Записать();','','13',0,'2013-12-04 12:12:00','2013-12-04 13:12:12',NULL,0,0,0,0,0,0,5,'',0,0,'',0,'',0,'0000-00-00 00:00:00','0000-00-00 00:00:00','','',''),(26,'Импорт фото и карты','',0,0,0,'// Вставить содержимое обработчика.\r\n	\r\n	строка=1; //строка в экселе с которой начинаются площадки\r\n	НомерКолонкиГиперСсылокФото = 14;\r\n	НомерКолонкиГиперСсылокКарты = 15;\r\n	НомерКолонкиКК = 1;\r\n	НомерКолСтор = 1;\r\n	//НомерКолГород=3;\r\n	НомерКолАдр = 4;\r\n	НомерКартинкиФотоНаСтранице=1;\r\n	Каталог=\"c:\\!Bases\\htdocs\\pic\\\"+подрядчик.код+\"\\\";\r\n	КаталогМап=\"c:\\!Bases\\htdocs\\pic\\\"+подрядчик.код+\"\\map\\\";\r\n	КаталогСервер=\"http://217.12.245.206:8080/pic/\"+подрядчик.код+\"/\";\r\n	КаталогСерверМап=\"http://217.12.245.206:8080/pic/\"+подрядчик.код+\"/map/\";\r\n	\r\n	КолЗагрузок=0;\r\n	КолСсылокБыло=0;\r\n	КолОшибок=0;\r\n	\r\n	//Создание каталога\r\n	создатькаталог(Каталог);\r\n	создатькаталог(КаталогМап);\r\n	\r\n	fso = Новый COMОбъект(\"Scripting.FileSystemObject\");	\r\n	\r\n	//Excel\r\n	Эксель = Новый COMОбъект(\"Excel.Application\");\r\n	Эксель.Visible = false;\r\n	Книга = Эксель.WorkBooks.Open(файл_импорта);\r\n	\r\n	Если Лист=\"\" Тогда\r\n		Лист=Книга.Sheets(1).Name;\r\n	КонецЕсли;\r\n	Лист1 = Книга.WorkSheets(Лист);\r\n	\r\n	ВсегоСтрок = Лист1.Cells(1,1).SpecialCells(11).Row;\r\n	//ВсегоСтрок=24;\r\n	\r\n	// Обработка нажатия Ctrl + Break\r\n	//ОбработкаПрерыванияПользователя();\r\n	\r\n	objIE = новый comобъект(\"InternetExplorer.Application\"); \r\n	objIE.Visible =0; 				\r\n	Пока Строка <= ВсегоСтрок цикл\r\n		КодК = Лист1.Cells(строка,  НомерКолонкиКК).Text;\r\n		//	КодК=СокрЛП(КодК);\r\n		//	КодК=СтрЗаменить(КодК, \" \", \"\");\r\n		//	КодК=СтрЗаменить(КодК, Символ(160), \"\");\r\n		\r\n		Сторона1 = Лист1.Cells(строка, НомерКолСтор).Value;\r\n		Если Найти(Сторона1,\" \")>1 Тогда\r\n			Сторона1 = Сред(Сторона1,Найти(Сторона1,\" \")+1);\r\n		КонецЕсли;\r\n		Сторона1 = СтрЗаменить(Сторона1,\"А\",\"A\");\r\n		Сторона1 = СтрЗаменить(Сторона1,\"Б\",\"B\");\r\n		Сторона1 = СтрЗаменить(Сторона1,\"С\",\"C\");\r\n		Если Сторона1=\"\" Тогда\r\n			Сторона= Справочники.стороны.ПустаяСсылка();\r\n		Иначе	\r\n			Сторона= НайтиСторону.НайтиСторону(Сторона1, Ложь);\r\n		КонецЕсли;\r\n		\r\n		Адрес = Лист1.Cells(строка, НомерКолАдр).Value;\r\n		\r\n		//п1 = РаспознаваниеПовторенийДляСсылок.ОпределитьСуществованиеСсылки(Подрядчик, КодК, Сторона, Адрес);\r\n		\r\n		//Если п1 = ложь тогда\r\n		//сообщить(п1);\r\n		ЕстьФото=Ложь;\r\n		ЕстьКарта=Ложь;\r\n		\r\n		//фото\r\n		АдрФото = Лист1.Cells(строка, НомерКолонкиГиперСсылокФото).formula;\r\n		текст = \"=HYPERLINK(\"\"\";\r\n		АдрФото = СтрЗаменить(АдрФото, текст,\"\");\r\n		текст = \"\"\",\"\"Ссылка\"\"\"+\")\";\r\n		АдрФото = СтрЗаменить(АдрФото, текст,\"\");\r\n		текст = \"show/?src_file=/\";\r\n		АдрФото = СтрЗаменить(АдрФото, текст,\"\");\r\n\r\n		//карта\r\n		АдрКарта = Лист1.Cells(строка, НомерКолонкиГиперСсылокКарта).formula;\r\n		текст = \"=HYPERLINK(\"\"\";\r\n		АдрКарта = СтрЗаменить(АдрКарта, текст,\"\");\r\n		текст = \"\"\",\"\"Ссылка\"\"\"+\")\";\r\n		АдрКарта = СтрЗаменить(АдрКарта, текст,\"\");\r\n		текст = \"show/?src_file=/\";\r\n		АдрКарта = СтрЗаменить(АдрКарта, текст,\"\");\r\n\r\n\r\n//=== ФОТО ===		\r\nФотоИмя=\"\";\r\n		Если найти(АдрФото,\"yadi.sk\")<>0 тогда\r\n		     //сообщить(\"АдрФото \"+АдрФото);\r\n			\r\n			objIE.Navigate(АдрФото);\r\n			\r\n			Пока objIE.readyState<4 Цикл \r\n			КонецЦикла;\r\n			\r\n			Пока objIE.Busy  Цикл\r\n			КонецЦикла;\r\n			\r\nпопытка\r\n			Картинка = objIE.Document.images(НомерКартинкиФотоНаСтранице).sr